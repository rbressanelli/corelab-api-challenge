{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "quotes": "off",
    "import/extensions": [
      "off",
      {
        "ts": "never"
      }
    ],
    "indent": "off",
    "no-return-await": "off",
    "prettier/prettier": ["error"]
  }
}
